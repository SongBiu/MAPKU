/**app.wxss**/
page {
	height: 100%;
	width:100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 0;
  box-sizing: border-box;
  width:100%;
} 
view.bottom {
	width: 100%;
	position: fixed;
	display: flex;
	flex-direction: row;
	bottom: 0;
	height: 12%;
	border-top: solid; 
	border-color: #BEBEBE;
	text-align: center;
}
.button-bottom {
  flex:1;
	height: 100%;
	display: flex;
	flex-direction: column;
	align-items: center;
  margin-top:0.4em;
  font-size: 0.8em;
}
view.button-bottom navigator {
	height: 100%;
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}
image.bottom {
  width: 30%;
  height: 0;
	display: inline-block;
}
#this {
	border-bottom: solid;
	border-color: green;
}
#this navigator {
	color: green;
}