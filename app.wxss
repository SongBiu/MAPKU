/**app.wxss**/
page {
	height: 100%;
	width:100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 0;
  box-sizing: border-box;
  width:100%;
} 
view.bottom {
	width: 100%;
	position: fixed;
	display: flex;
	flex-direction: row;
	bottom: 0;
	height: 11%;
	border-top: solid; 
	border-color: #BEBEBE;
}
.button-bottom {
	width: 33%;
	height: 100%;
	display: flex;
	flex-direction: column;
	align-items: center;
}
view.button-bottom navigator {
	height: 100%;
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}
image.bottom {
	height: 84%;
	width: 33%;
	display: inline-block;
}