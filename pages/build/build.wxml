<!--pages/build/build.wxml-->
<view class='container'>
	<form bindsubmit='formSubmit'>
		<text>请输入社团名称（要求写学校全称，比如“北京大学山鹰社”不应该写“北大山鹰社”</text>
		<textarea class='communityName' name="communityName" ></textarea>
		<text>请简单描述社团（200字以内）</text>
		<textarea class="desc" name="desc"></textarea>
		<text>请填写社团负责人姓名</text>
		<textarea class='name' name="name"></textarea>
		<text>请填写社团负责人电话</text>
		<textarea class='phone' name="phone"></textarea>
		<text>请填写社团联系邮箱</text>
		<textarea class='mail' name="mail"></textarea>
		<navigator url='../pages/communityRule/communityRule'></navigator>
		<label>我已阅读环保活动社团创建协议，并承诺会遵守该协议</label>
		<button form-type='submit'>提交</button>
	</form>
</view>
