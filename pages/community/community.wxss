/* pages/community/community.wxss */
.table {
	width: 100%;
	border: 0px solid darkgray;
	position: absolute;
	top:5%;
	height:85%;
	overflow-y: scroll;
}

.tr {
	display: flex;
	width: 100%;
	justify-content: center;
	height: 3rem;
	align-items: center;
}

.td {
	width: 100%;
	justify-content: center;
	text-align: center;
}

.bg-w {
	background: snow;
}

.bg-g {
	background: #e6f3f9;
}

.th {
	width: 100%;
	justify-content: center;
	background: #36f;
	color: #fff;
	display: flex;
	height: 3rem;
	align-items: center;
}

view.topline {
	display: flex;
	flex-direction: row;
	position: fixed;
	top:0;
	width: 100%;
	height: 7%;
	align-items: center;
}
view.btn {
	width: 33%;
	height: 100%;
	align-items: center;
	justify-content: center;
	align-content: center;
}
view.btn navigator {
	text-align: center;
	height: 100%;
	display: flex;
	justify-content: center;
	color: #1C1C1C;
}
