            </navigator>

        </view>
        <view class='button-bottom'>
            <navigator url='../upload/upload'>
                <button>发布</button>
            </navigator>

        </view>
        <view class='button-bottom'>
            <navigator url='../my/my'>
                <button>我的</button>
            </navigator>

        </view>
    </view>
</view>