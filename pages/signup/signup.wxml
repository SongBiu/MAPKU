<!--pages/signup/signup.wxml-->
<view class='container'>
    <image src='{{app.url_pre}}/icon/copy.png' class="top"></image>
    <form bindsubmit='formSubmit'>
        <input name="name" class='name' placeholder="昵称" />
        <input name="email" class='mail' placeholder="联系邮箱"/>
        <input name="invitateCode" class='invitateCode' placeholder="验证码（选填）" />
    </form>
</view>